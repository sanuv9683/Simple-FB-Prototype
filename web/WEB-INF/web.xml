<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>a</servlet-name>
        <servlet-class>web.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>a</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>lo</servlet-name>
        <servlet-class>web.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lo</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>b</servlet-name>
        <servlet-class>web.FriendRequestHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>b</servlet-name>
        <url-pattern>/friend</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>m</servlet-name>
        <servlet-class>web.MessageSend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>m</servlet-name>
        <url-pattern>/message</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>c</servlet-name>
        <servlet-class>web.chat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>c</servlet-name>
        <url-pattern>/chat</url-pattern>
    </servlet-mapping>
    
</web-app>
